# Mistral AI API Configuration (for OCR extraction from images and PDFs)
MISTRAL_API_KEY=your-mistral-api-key

# OpenAI API Configuration (for notarial text processing)
OPENAI_API_KEY=your-openai-api-key

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_MAX_FILE_SIZE=10485760

# OCR Debug Mode (test OCR extraction without AI processing)
# Set to 'true' to test OCR accuracy improvements
OCR_DEBUG_MODE=false

# Optional: Supabase Configuration (for future backend integration)
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
